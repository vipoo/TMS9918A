10 REM BARNSLEY FERN FOR MSX-BASIC
20 SCREEN 2: COLOR 2,0,0: CLS
30 X=0: Y=0
40 P=RND(1)*100
50 IF P<=1 THEN NX=0: NY=0.16*Y: GOTO 90
60 IF P<=8 THEN NX=0.2*X-0.26*Y: NY=0.23*X+0.22*Y+1.6: GOTO 90
70 IF P<=15 THEN NX=-0.15*X+0.28*Y: NY=0.26*X+0.24*Y+0.44: GOTO 90
80 NX=0.85*X+0.04*Y: NY=-0.04*X+0.85*Y+1.6
90 X=NX: Y=NY: PSET(127+X*17,190-Y*17)
100 IF INKEY$="" THEN 40